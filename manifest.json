{
  "manifest_version": 2,
  "name": "chess stats",
  "version": "1.0",
  "description": "Adds extra statistics panel to chess.stjo.dev's bots",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://chess.stjo.dev/bot*"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_end"
    }
  ]
}
